# Application Configuration (Production)
# Vercel will automatically provide the deployment URL
# NEXT_PUBLIC_APP_URL will be set automatically by Vercel
NODE_ENV=production

# Admin Configuration (Production)
# Use a strong password for production
ADMIN_PASSWORD=your_secure_admin_password_here

# Debug Mode (Production)
# Disable debug mode for production
NEXT_PUBLIC_DEBUG_MODE=false

# Additional Production Settings
# Enable production optimizations
NEXT_PUBLIC_APP_ENV=production

# Security headers and CORS
NEXT_PUBLIC_CORS_ORIGINS=https://your-mighty-team-designs.railway.app

# Database connection pool settings for production
DATABASE_POOL_SIZE=10
DATABASE_POOL_TIMEOUT=30000

# Logging configuration for production
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# Rate limiting for production
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_WINDOW_MS=60000

# File upload limits for production
MAX_FILE_SIZE_MB=10
MAX_FILES_PER_REQUEST=5

# Cache settings for production
CACHE_TTL_SECONDS=3600
ENABLE_REDIS_CACHE=false

# Monitoring and analytics
ENABLE_ANALYTICS=true
SENTRY_DSN=your_sentry_dsn_here_if_using
