{
  "name": "party-game",
  "packageManager": "pnpm@8.15.0",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "migrate": "pnpm --filter api prisma migrate dev",
    "seed": "pnpm --filter api prisma db seed"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.5.0",
    "@types/node": "^20.12.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.5.0",
      "eslint": "^9.0.0"
    }
  }
}
